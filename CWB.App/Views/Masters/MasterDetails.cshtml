<template id="MasterDetaiTemplate">
    <tr style="text-align: center;">
        <td data-key="Company">{company}</td>
        <td data-key="PartNo">{partNo}</td>
        <td data-key="Description">{description}</td>
        <td data-key="MasterDisplay">{masterDisplay}</td>
        <td data-key="BomAvl">{bomAvl}</td>
        <td data-key="RmAvl">{rmAvl}</td>
        <td data-key="SupplierAvl">{supplierAvl}</td>
        <td data-key="MandocAvl">{mandocAvl}</td>
        <td data-key="Status" hidden>{status}</td>
        <td data-key="FinalPart" hidden>{finalPart}</td>
        <td data-key="DocStatus">{docStatus}</td>
        <td>
            <div class="dropdown float-center">
                <a href="#" class="dropdown-toggle arrow-none card-drop" data-bs-toggle="dropdown" aria-expanded="false"><i class="mdi mdi-dots-vertical"></i></a>
                <div class="dropdown-menu dropdown-menu-end">
                    <!-- item-->
                    @if (permissionResult != null)
                    {
                        @if (permissionResult.Any(r => r.Menu3 == "View / Edit Part No"))
                        {
                            var pr = permissionResult.Where(r => r.Menu3 == "View / Edit Part No").FirstOrDefault();
                            if (pr.PermissionId >= 3)
                            {
                                <a href="/masters/editpart?partId={partId}&partType={masterPartType}">Edit Part Details</a>
                            }
                            else
                            {
                            }
                        }


                    }
                    else
                    {
                        <a href="/masters/editpart?partId={partId}&partType={masterPartType}">Edit Part Details</a>
                    }
                </div>
            </div>
        </td>
    </tr>
</template>

<template id="MasterDocRow">

    <tr>
        <td>{documentTypeName}</td>
        <td>{mandatory}</td>
        <td>
            <div class="dropdown float-center">
                <a href="#" class="dropdown-toggle arrow-none card-drop" data-bs-toggle="dropdown" aria-expanded="false"><i class="mdi mdi-dots-vertical"></i></a>
                <div class="dropdown-menu dropdown-menu-end">
                    <!-- item-->
                    <a href="javascript:void(0);" class="dropdown-item" data-docname="{documentTypeId}" data-mandatory="{mandatory}" data-mastercontent="{contentId}"
                       data-masterdocid="{itemMasterDocListId}" onclick="EditItemMasterDoc(this)">Edit</a>
                    <!-- item-->
                    <a href="javascript:void(0);" class="dropdown-item"
                       data-docname="{documentTypeId}" data-mandatory="{mandatory}" data-mastercontent="{contentId}"
                       data-masterdocid="{itemMasterDocListId}" onclick="DeleteItemMasterDoc(this)">Delete</a>
                </div>
            </div>
        </td>
    </tr>
</template>

