@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "Item Master";
}
<input value="M-IM" type="hidden" id="hdn-nav-menu" />
<div class="content">
    <div class="row" id="myRadioGroup">
        <div class="col-xl-12 col-md-12 col-sm-12 col-xs-12">
            <!-- ================= -->
            @{
                List<CWB.App.Models.EmployeeMaster.Role_UI_ListVM> permissionResult = null;
                var permissionsJson = HttpContextAccessor.HttpContext.Session.GetString("Permissions");
                if (permissionsJson != null)
                {
                    permissionResult = Newtonsoft.Json.JsonConvert.DeserializeObject<List<CWB.App.Models.EmployeeMaster.Role_UI_ListVM>>(permissionsJson);

                }
            }
            <div class="card">
                <div class="card-body">
                    <table class="w-100 mb-2">
                        <tbody>
                            <tr>
                                <td>
                                    <h5>View / Edit Item Master</h5>
                                </td>
                                <td class="text-end">
                                    <a asp-action="Index" asp-controller="Masters"><button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <!-- =====================  -->
                    <div class="row mb-3">
                        <!-- end col -->
                        <label class="form-label col-md-1 mb-2">Part Type </label>
                        <div class="col-md-3 mb-2">
                            <select class="form-select form-select-sm" id="MasterPart" name="MasterPart" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select Part No from Drop Down">
                                <option id="mp-se" value="0">--Select--</option>
                                <option id="mp-mp" value="1">Manufactured Part</option>
                                <option id="mp-as" value="2">Assembly</option>
                                <option id="mp-bf" value="3">BOF</option>
                                <option id="mp-rm" value="4">Raw Material</option>
                            </select>
                        </div>
                        <!-- end col -->
                        <label class="form-label col-md-1 mb-2">Company</label>
                        <div class="col-md-3 mb-2">
                            <input class="form-control form-control-sm" type="text" id="master_co" name="master_co" placeholder="enter here">
                        </div>
                        <!-- -------------------------------------  -->
                        <!-- end col -->
                        <label class="form-label col-md-1 mb-2">Part Status </label>
                        <div class="col-md-3 mb-2">
                            <select class="form-select form-select-sm" id="Status" name="Status" asp-items="ViewBag.Statuses" required>
                            </select>
                        </div>
                        <!-- =============== -->
                        <label class="form-label col-md-1 mb-2">Part No </label>
                        <div class="col-md-3 mb-2">
                            <input class="form-control form-control-sm" type="text" id="master_partno" name="master_partno" placeholder="enter here">
                        </div>
                        <!-- =============== -->
                        <label class="form-label col-md-1 mb-2">Part Desc </label>
                        <div class="col-md-3 mb-2">
                            <textarea name="master_description" form="usrform" id="master_description" class="form-control form-control-sm" title="Enter Part Description - full or partial to shortlist Part Nos" data-plugin="tippy" data-tippy-placement="top"></textarea>
                        </div>
                        <label class="form-label col-md-1 mb-2">Docu Status </label>
                        <div class="col-md-3 mb-2">
                            <select class="form-select form-select-sm" id="DocRefStatus">
                                <option value="0">--Select--</option>
                                <option value="1">Approved</option>
                                <option value="2">Waiting for Approval</option>
                                <option value="3">Not Approved</option>
                                <option value="4">N/A</option>
                            </select>
                        </div>
                        <!-- =============== -->
                        <!-- end col -->
                    </div>
                    <!-- ---- end row ----  -->
                    <div class="row mb-3">
                        <div class="col-md-10">
                            <table class="w-100">
                                <tbody data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select the category of the Part">
                                    <tr>
                                        <td>
                                            <label class="form-check-label" for="cars">Manufactured Part without RM </label>
                                            <input class="form-check-input mt-1" type="radio" id="manfWithOutRM" name="cars" value="2">
                                        </td>
                                        <td>
                                            <label class="form-check-label" for="cars">Assembly Part No without BOM </label>
                                            <input class="form-check-input mt-1" type="radio" id="AssWithOuutBOM" name="cars" value="3">
                                        </td>
                                        <td>
                                            <label class="form-check-label" for="cars">
                                                Purchased Matl without Supplier
                                            </label>
                                            <input class="form-check-input mt-1" type="radio" id="PurcSupplier" name="cars" value="4">
                                        </td>
                                        <td>
                                            <label class="form-check-label" for="cars">Mandatory Docs not Uploaded </label>
                                            <input class="form-check-input mt-1" type="radio" id="MandatoryNotUp" name="cars" value="5">
                                        </td>
                                        <td>
                                            <label class="form-check-label" for="cars">
                                                Final Part No Sold to
                                                Customer
                                            </label>
                                            <input class="form-check-input mt-1" type="radio" id="finalepartSold" name="cars" value="5">
                                        </td>
                                        <td>
                                            <label class="form-check-label" for="cars">
                                                &nbsp;&nbsp;
                                            </label>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="col-md-2 text-end">
                            <button type="button" class="btn btn-outline-primary btn-sm mt-3"
                                    id="ClearSearchFields">
                                Clear Fields
                            </button>
                        </div>
                    </div>
                    <!-- ---- end row ----  -->
                    <!-- ===== start-1st ====== -->
                    <div class="row desc" id="Cars2" style="display:none">
                        <div class="col-md-3">
                            <table class="w-100">
                                <tbody>
                                    <tr>
                                        <td data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Check the Box to see Manufactured Part Nos without Routing">
                                            <label class="form-check-label" for="gp-4">
                                                Show Part Numbers Without
                                                Routing
                                            </label>
                                            <input class="form-check-input mt-1" type="checkbox" name="gp-4">
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- ===== end-1st ====== -->
                    <!-- ===== start-2nd ====== -->
                    <div class="row desc" id="Cars3" style="display:none">
                        <div class="col-md-3" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Check the Box to see Manufactured Part Nos without Routing">
                            <label class="form-check-label" for="gp-5">Show Part Numbers Without Routing</label>
                            <input class="form-check-input mt-1" type="checkbox" name="gp-5">
                        </div>
                        <div class="col-md-3 text-end" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Check the Box to see Assembly Part Nos without BOM">
                            <label class="form-check-label" for="gp-5">Show Part Numbers Without BOM</label>
                            <input class="form-check-input mt-1" type="checkbox" name="gp-5">
                        </div>
                    </div>
                    <!-- ===== end-2nd ====== -->
                    <!-- ===== start-3rd ====== -->
                    <div class="row desc" id="Cars4" style="display:none">
                        <label class="form-label col-md-2 mb-2">BOF Type</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select BOF Type from Drop Down">
                            <select class="form-select form-select-sm">
                                <option value="">Standard</option>
                                <option value="">Catalog</option>
                                <option value=""> Made to Print </option>
                            </select>
                        </div>
                        <div class="col-md-6"></div>
                        <!-- ----------------  -->
                        <label class="form-label col-md-2 mb-2">Supplier</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select Supplier Company from Drop Down">
                            <select class="form-select form-select-sm">
                                <option value="">Select Supplier</option>
                                <option value="">XXXXXXX</option>
                                <option value="">XXXXX</option>
                            </select>
                        </div>
                        <!-- ----------------  -->
                        <label class="form-label col-md-2 mb-2">Supplier Part No</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Enter Supplier Part No">
                            <input class="form-control form-control-sm" type="text" placeholder="enter here">
                        </div>
                        <!-- ------------------- -->
                    </div>
                    <!-- ===== end-3rd ====== -->
                    <!-- ===== start-4th ====== -->
                    <div class="row desc" id="Cars5" style="display:none">
                        <!-- ------------------- -->
                        <label class="form-label col-md-2 mb-2"> RM Category</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select Raw Material (RM) Category from Drop Down (Standard RM, Made to Print ...)">
                            <select class="form-select form-select-sm">
                                <option value=""> Made to Print </option>
                                <option value=""> Standard Purchased RM </option>
                            </select>
                        </div>
                        <!-- ------------------- -->
                        <label class="form-label col-md-2 mb-2"> RM Type</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select Raw Material Type from Drop Down (Casting, Forging ...)">
                            <select class="form-select form-select-sm">
                                <option value="">Forging</option>
                                <option value="">Casting</option>
                                <option value="">Sheet</option>
                                <option value="">Bar Stock</option>
                            </select>
                        </div>
                        <!-- ------------------- -->
                        <label class="form-label col-md-2 mb-2"> Supplier</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select Supplier Company from Drop Down">
                            <select class="form-select form-select-sm">
                                <option value="">Select Supplier</option>
                                <option value="">XXXXXXX</option>
                                <option value="">XXXXX</option>
                            </select>
                        </div>
                        <!-- ------------------- -->
                        <label class="form-label col-md-2 mb-2">Supplier Part No</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Enter Supplier Part No">
                            <input class="form-control form-control-sm" type="text" placeholder="enter here">
                        </div>
                        <!-- ------------------- -->
                        <label class="form-label col-md-2 mb-2"> Base Material</label>
                        <div class="col-md-4 mb-2" data-plugin="tippy" data-tippy-placement="top" data-tippy="" data-original-title="Select Base Material from Drop Down (Steel, Aluminium, Brass ...)">
                            <select class="form-select form-select-sm">
                                <option value="">XXXXX</option>
                                <option value="">XXXXXX</option>
                            </select>
                        </div>
                        <!-- ------------------- -->
                    </div>
                    <!-- ===== end-4th ====== -->
                </div>
            </div>
            <!-- end card body -->
            <!-- ============================= -->
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="table-responsive mt-1 table-he-180">
                            <table class="table table-sm table-bordered w-100 mb-3 tableFixHead" id="mptable">
                                <thead class="  table-info th-sti text-center">
                                    <tr class="table-border-bottom">
                                        <th>Company</th>
                                        <th>Part No</th>
                                        <th>Description</th>
                                        <th>Part Type</th>
                                        <th>BOM Avl</th>
                                        <th>RM Avl</th>
                                        <th>Supplier Avl</th>
                                        <th>Mandatory Doc Avl</th>
                                        <th>Docu Status</th>
                                        <th width="5%"></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!-- --------------------  -->
                                    <!--<tr>
                <td>XXXXXXXXX</td>
                <td>XXXXXXXXX</td>
                <td>XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX</td>
                <td> Manufactured Part without Routing</td>
                <td>N/A</td>
                <td>Yes</td>
                <td>N/A</td>
                <td>No</td>
                <td>
                    <div class="dropdown float-center">
                        <a href="#" class="dropdown-toggle arrow-none card-drop" data-bs-toggle="dropdown" aria-expanded="false"><i class="mdi mdi-dots-vertical"></i></a>
                        <div class="dropdown-menu dropdown-menu-end">-->
                                    <!-- item-->
                                    <!--<a href="#" class="dropdown-item">Edit Part Details</a>-->
