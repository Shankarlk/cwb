                                    <!-- item-->
                                    <!--<a href="bom.html" class="dropdown-item">Create BOM</a>-->
                                    <!-- item-->
                                    <!--<a href="routing.html" class="dropdown-item">Create Routing</a>-->
                                    <!-- item-->
                                    <!--<a href="manf-part.html" class="dropdown-item">Create Balloon</a>-->
                                    <!-- item-->
                                    <!--<a href="manf-part.html" class="dropdown-item">Edit Balloon</a>
                            </div>
                        </div>
                    </td>
                </tr>-->
                                    <!-- --------------------  -->
                                    <!-- --------------------  -->
                                </tbody>
                            </table>
                        </div>
                        <!-- =========================== -->
                        @if (permissionResult != null)
                        {
                            @if (permissionResult.Any(r => r.Menu3 == "View / Edit Part No"))
                            {
                                var pr = permissionResult.Where(r => r.Menu3 == "View / Edit Part No").FirstOrDefault();
                                if (pr.PermissionId >= 3)
                                {
                                    <div class="col-md-12 mt-2">
                                        <button class="btn btn-purple btn-sm" data-bs-toggle="modal" data-bs-target="#adtimc">
                                            Associated Document Type with Item Master
                                            Content
                                        </button>
                                    </div>
                                }
                                else
                                {
                                    <div class="col-md-12 mt-2">
                                        <button class="btn btn-purple btn-sm" data-bs-toggle="modal" data-bs-target="#" disabled>
                                            Associated Document Type with Item Master
                                            Content
                                        </button>
                                    </div>
                                }
                            }


                        }
                        else
                        {
                            <div class="col-md-12 mt-2">
                                <button class="btn btn-purple btn-sm" data-bs-toggle="modal" data-bs-target="#adtimc">
                                    Associated Document Type with Item Master
