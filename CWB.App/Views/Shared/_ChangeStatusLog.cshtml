<div class="modal fade" id="changelog" role="dialog"  data-backdrop=" static" data-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content  bxs-modal">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Routing Change Status Log</h5>
                <button id="changelogClose" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12 mb-2">
                        <h5>Part No / Desc : <span id="SCSPartName"> </span> /  <span id="SCSPartDesc"> </span></h5>
                        <h6>Routing Name : <span id="SCSRoutingName"> </span></h6>
                    </div>
                    <!-- ---------------  -->
                    <div class="col-md-12">
                        <div class="table-responsive mt-2 table-bordered table-he-180 ">
                            <table class="table table-sm table-bordered  mb-3 text-center mt-1 tableFixHead" id="CSLogGrid">
                                <thead class="table-info th-sti">
                                    <tr class="table-border-bottom">
                                        <th>Date</th>
                                        <th>By</th>
                                        <th>Prev Status</th>
                                        <th>Changed Status</th>
                                        <th>Reason</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- ---------------  -->
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<template id="CSLogRow">
    <tr>
        <td>{dateStr}</td>
        <td>{updatedByStr}</td>
        <td>{prevStatus}</td>
        <td>{changedStatus}</td>
        <td>{reason}</td>
    </tr>
</template>